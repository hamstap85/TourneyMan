{
  "name": "TourneyMan",
  "version": "0.1.4",
  "description": "A tournament management system for organized play.",
  "main": "main.js",
  "scripts": {
    "start": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitanata/TourneyMan"
